<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.BuscarPorVeiculo.ListaInspecaoVeiculosActivity"
    android:orientation="vertical"
    android:background="#58C3BF">

    <TextView
        android:id="@+id/textView12"
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:background="@color/fundoimpar"
        android:gravity="center"
        android:text="A seguir você encontra uma\n lista com todos as inspeções"
        android:textAlignment="center"
        android:textColor="#111"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/inspecoesVeiculosListView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ListView
        android:id="@+id/inspecoesVeiculosListView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFF" />

</LinearLayout>