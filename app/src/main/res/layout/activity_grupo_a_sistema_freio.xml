<?xml version="1.0" encoding="utf-8"?>
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".activity.GrupoA.GrupoASistemaFreioActivity">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/fundoimpar"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textViewTituloGrupoA"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:text="Grupo A"
                        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
                        android:textSize="15sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/textViewSistemaFreio"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:text="1 - Sistemas de Freio"
                        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                        android:textSize="15sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/fundopar"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textViewValvulaPedal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="5dp"
                        android:gravity="fill_horizontal|center"
                        android:text="Válvula Pedal"
                        android:textColor="#111"
                        android:textIsSelectable="false"
                        android:textSize="15sp"
                        android:textStyle="bold"
                        tools:style="@android:style/Widget.DeviceDefault.TextView" />

                    <CheckBox
                        android:id="@+id/valvulaPedalVazando"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Vazando"
                        android:layout_marginLeft="15dp"/>

                    <CheckBox
                        android:id="@+id/valvulaPedalContaminada"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Contaminada"
                        android:layout_marginLeft="15dp"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/fundoimpar"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textViewAlmofadaPedal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="5dp"
                        android:gravity="fill_horizontal|center"
                        android:text="Almofada Pedal"
                        android:textColor="#111"
                        android:textIsSelectable="false"
                        android:textSize="15sp"
                        android:textStyle="bold"
                        tools:style="@android:style/Widget.DeviceDefault.TextView" />

                    <CheckBox
                        android:id="@+id/almofadaPedalGasta"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Gasta"
                        android:layout_marginLeft="15dp"/>

                    <CheckBox
                        android:id="@+id/almofadaPedalFalta"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Falta"
                        android:layout_marginLeft="15dp"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/fundopar"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textViewFreioEstacionamento"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="5dp"
                        android:gravity="fill_horizontal|center"
                        android:text="Freio de Estacionamento"
                        android:textColor="#111"
                        android:textIsSelectable="false"
                        android:textSize="15sp"
                        android:textStyle="bold"
                        tools:style="@android:style/Widget.DeviceDefault.TextView" />

                    <CheckBox
                        android:id="@+id/freioEstacionamentoNaoFunciona"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Não Funciona"
                        android:layout_marginLeft="15dp"/>

                    <CheckBox
                        android:id="@+id/freioEstacionamentoVazando"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Vazando"
                        android:layout_marginLeft="15dp"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/fundoimpar"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textViewVaraoFreioMao"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="5dp"
                        android:gravity="fill_horizontal|center"
                        android:text="Varão do Freio de Mão"
                        android:textColor="#111"
                        android:textIsSelectable="false"
                        android:textSize="15sp"
                        android:textStyle="bold"
                        tools:style="@android:style/Widget.DeviceDefault.TextView" />

                    <CheckBox
                        android:id="@+id/varaoFreioMaoSemAcao"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Sem Ação"
                        android:layout_marginLeft="15dp"/>

                    <CheckBox
                        android:id="@+id/varaoFreioMaoSolta"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Solta"
                        android:layout_marginLeft="15dp"/>

                    <CheckBox
                        android:id="@+id/varaoFreioMaoDespressurizada"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Despressurizada"
                        android:layout_marginLeft="15dp"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/fundopar"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textViewCatracas"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="5dp"
                        android:gravity="fill_horizontal|center"
                        android:text="Catracas Automática/Mecânica"
                        android:textColor="#111"
                        android:textIsSelectable="false"
                        android:textSize="15sp"
                        android:textStyle="bold"
                        tools:style="@android:style/Widget.DeviceDefault.TextView" />

                    <CheckBox
                        android:id="@+id/catracaFalta"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Falta"
                        android:layout_marginLeft="15dp"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/fundoimpar"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textViewPinoCatraca"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="5dp"
                        android:gravity="fill_horizontal|center"
                        android:text="Pino da Catraca"
                        android:textColor="#111"
                        android:textIsSelectable="false"
                        android:textSize="15sp"
                        android:textStyle="bold"
                        tools:style="@android:style/Widget.DeviceDefault.TextView" />

                    <CheckBox
                        android:id="@+id/pinoCatracaFalta"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Falta"
                        android:layout_marginLeft="15dp"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/fundopar"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textViewLonasFreio"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="5dp"
                        android:gravity="fill_horizontal|center"
                        android:text="Lonas de Freio"
                        android:textColor="#111"
                        android:textIsSelectable="false"
                        android:textSize="15sp"
                        android:textStyle="bold"
                        tools:style="@android:style/Widget.DeviceDefault.TextView" />

                    <CheckBox
                        android:id="@+id/lonasFreioQuebrada"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Quebrada"
                        android:layout_marginLeft="15dp"/>

                    <CheckBox
                        android:id="@+id/lonasFreioContaminada"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Contaminada"
                        android:layout_marginLeft="15dp"/>

                    <CheckBox
                        android:id="@+id/lonasFreioFina"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Fina"
                        android:layout_marginLeft="15dp"/>

                    <CheckBox
                        android:id="@+id/lonasFreioSolta"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Solta"
                        android:layout_marginLeft="15dp"/>

                    <CheckBox
                        android:id="@+id/lonasFreioDesrregulada"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Desrregulada"
                        android:layout_marginLeft="15dp"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/fundoimpar"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textViewCilindrosPneumaticos"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="5dp"
                        android:gravity="fill_horizontal|center"
                        android:text="Cilindros Pneumáticos"
                        android:textColor="#111"
                        android:textIsSelectable="false"
                        android:textSize="15sp"
                        android:textStyle="bold"
                        tools:style="@android:style/Widget.DeviceDefault.TextView" />

                    <CheckBox
                        android:id="@+id/cilindrosPneumaticosVazando"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Vazando"
                        android:layout_marginLeft="15dp"/>

                    <CheckBox
                        android:id="@+id/cilindrosPneumaticosDesativado"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Desativado"
                        android:layout_marginLeft="15dp"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/fundopar"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textViewServoFreio"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="5dp"
                        android:gravity="fill_horizontal|center"
                        android:text="Servo do Freio"
                        android:textColor="#111"
                        android:textIsSelectable="false"
                        android:textSize="15sp"
                        android:textStyle="bold"
                        tools:style="@android:style/Widget.DeviceDefault.TextView" />

                    <CheckBox
                        android:id="@+id/servoFreioVazando"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Vazando"
                        android:layout_marginLeft="15dp"/>

                    <CheckBox
                        android:id="@+id/servoFreioSolto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Solto"
                        android:layout_marginLeft="15dp"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/fundoimpar"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textViewCilindroRoda"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="5dp"
                        android:gravity="fill_horizontal|center"
                        android:text="Cilindro de Roda"
                        android:textColor="#111"
                        android:textIsSelectable="false"
                        android:textSize="15sp"
                        android:textStyle="bold"
                        tools:style="@android:style/Widget.DeviceDefault.TextView" />

                    <CheckBox
                        android:id="@+id/cilindroRodaSolto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Solto"
                        android:layout_marginLeft="15dp"/>

                    <CheckBox
                        android:id="@+id/cilindroRodaVazando"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Vazando"
                        android:layout_marginLeft="15dp"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/fundopar"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textViewCilindroMestre"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="5dp"
                        android:gravity="fill_horizontal|center"
                        android:text="Cilindro Mestre"
                        android:textColor="#111"
                        android:textIsSelectable="false"
                        android:textSize="15sp"
                        android:textStyle="bold"
                        tools:style="@android:style/Widget.DeviceDefault.TextView" />

                    <CheckBox
                        android:id="@+id/cilindroMestreVazando"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Vazando"
                        android:layout_marginLeft="15dp"/>

                    <CheckBox
                        android:id="@+id/cilindroMestreSolto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Solto"
                        android:layout_marginLeft="15dp"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/fundoimpar"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textViewFlexivelRoda"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="5dp"
                        android:gravity="fill_horizontal|center"
                        android:text="Flexível da Roda"
                        android:textColor="#111"
                        android:textIsSelectable="false"
                        android:textSize="15sp"
                        android:textStyle="bold"
                        tools:style="@android:style/Widget.DeviceDefault.TextView" />

                    <CheckBox
                        android:id="@+id/flexivelRodaDanificado"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Danificado"
                        android:layout_marginLeft="15dp"/>

                    <CheckBox
                        android:id="@+id/flexivelRodaDesalinhado"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Desalinhado"
                        android:layout_marginLeft="15dp"/>

                    <CheckBox
                        android:id="@+id/flexivelRodaVazando"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Vazando"
                        android:layout_marginLeft="15dp"/>

                    <CheckBox
                        android:id="@+id/flexivelRodaIrregular"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Irregular"
                        android:layout_marginLeft="15dp"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/fundopar"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textViewValvulaTubularReservatorio"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="5dp"
                        android:gravity="fill_horizontal|center"
                        android:text="Válvulas Tubular Reservatório"
                        android:textColor="#111"
                        android:textIsSelectable="false"
                        android:textSize="15sp"
                        android:textStyle="bold"
                        tools:style="@android:style/Widget.DeviceDefault.TextView" />

                    <CheckBox
                        android:id="@+id/valvulaTubularReservatorioVazando"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Vazando"
                        android:layout_marginLeft="15dp"/>

                    <CheckBox
                        android:id="@+id/valvulaTubularReservatorioContaminada"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Contaminada"
                        android:layout_marginLeft="15dp"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:background="@color/fundopar"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/botaoVoltarSelecionarFicha"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Voltar"
                        android:background="@color/fundoimpar"
                        android:layout_margin="10dp"/>

                    <Button
                        android:id="@+id/botaoProximo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Próximo"
                        android:background="@color/fundoimpar"
                        android:layout_margin="10dp"/>

                </LinearLayout>

            </LinearLayout>

        </ScrollView>

    </LinearLayout>