                android:textSize="15sp"
                android:textStyle="bold"
                tools:layout_marginLeft="30dp"
                tools:style="@android:style/Widget.DeviceDefault.TextView" />

            <CheckBox
                android:id="@+id/algemaDanificado"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Danificado" />

            <CheckBox
                android:id="@+id/algemaSolto"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Solto" />

            <TextView
                android:id="@+id/textViewPinoSuporteMola"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="10dp"
                android:gravity="fill_horizontal|center"
                android:text="Pino Suporte Mola"
                android:textIsSelectable="false"
                android:textSize="15sp"
                android:textStyle="bold"
                tools:layout_marginLeft="30dp"
                tools:style="@android:style/Widget.DeviceDefault.TextView" />

            <CheckBox
                android:id="@+id/pinoSuporteMolaSolto"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Solto" />

            <CheckBox
                android:id="@+id/pinoSuporteMolaQuebrado"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Quebrado" />

            <CheckBox
                android:id="@+id/pinoSuporteMolaFalta"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Falta" />

            <TextView
                android:id="@+id/textViewMolaHelicoidal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="10dp"
                android:gravity="fill_horizontal|center"
                android:text="Mola Helicoidal"
                android:textIsSelectable="false"
                android:textSize="15sp"
                android:textStyle="bold"
