                tools:style="@android:style/Widget.DeviceDefault.TextView" />

            <CheckBox
                android:id="@+id/suporteAmortecedorDanificado"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Danificado" />

            <CheckBox
                android:id="@+id/suporteAmortecedorSolto"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Solto" />

            <TextView
                android:id="@+id/textViewBuchaAmortecedor"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="10dp"
                android:gravity="fill_horizontal|center"
                android:text="Bucha Amortecedor"
                android:textIsSelectable="false"
                android:textSize="15sp"
                android:textStyle="bold"
