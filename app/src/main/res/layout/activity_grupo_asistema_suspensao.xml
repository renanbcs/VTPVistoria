            <CheckBox
                android:id="@+id/hasteSuporteReacaoTraseiraFolga"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Folga" />

            <CheckBox
                android:id="@+id/hasteSuporteReacaoTraseiraSolta"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Solta" />

            <CheckBox
