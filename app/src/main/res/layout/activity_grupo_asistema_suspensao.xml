                tools:layout_marginLeft="30dp"
                tools:style="@android:style/Widget.DeviceDefault.TextView" />

            <CheckBox
                android:id="@+id/buchaMolasFolga"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Folga" />

            <TextView
                android:id="@+id/textViewEspigaoMolas"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="10dp"
                android:gravity="fill_horizontal|center"
                android:text="Espigão Molas"
                android:textIsSelectable="false"
                android:textSize="15sp"
                android:textStyle="bold"
                tools:layout_marginLeft="30dp"
                tools:style="@android:style/Widget.DeviceDefault.TextView" />

            <CheckBox
                android:id="@+id/espigaoMolasCortado"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Cortado" />

            <CheckBox
                android:id="@+id/espigaoMolasQuebrado"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Quebrado" />

            <TextView
                android:id="@+id/textViewGrampoMolas"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="10dp"
                android:gravity="fill_horizontal|center"
                android:text="Grampo Molas"
                android:textIsSelectable="false"
                android:textSize="15sp"
                android:textStyle="bold"
                tools:layout_marginLeft="30dp"
                tools:style="@android:style/Widget.DeviceDefault.TextView" />

            <CheckBox
                android:id="@+id/grampoMolasDanificado"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Danificado" />

            <CheckBox
                android:id="@+id/grampoMolasSolto"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Solto" />

            <TextView
                android:id="@+id/textViewSuporteMolas"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="10dp"
                android:gravity="fill_horizontal|center"
                android:text="Suporte Molas"
                android:textIsSelectable="false"
                android:textSize="15sp"
                android:textStyle="bold"
