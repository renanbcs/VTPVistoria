                tools:style="@android:style/Widget.DeviceDefault.TextView" />

            <CheckBox
                android:id="@+id/molaHelicoidalQuebrada"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Quebrada" />

            <CheckBox
                android:id="@+id/molaHelicoidalSolta"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Solta" />

            <TextView
                android:id="@+id/textViewSuporteParafusoMolaHelicoidal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="10dp"
                android:gravity="fill_horizontal|center"
                android:text="Suporte Parafuso Mola Helicoidal"
                android:textIsSelectable="false"
                android:textSize="15sp"
                android:textStyle="bold"
                tools:layout_marginLeft="30dp"
                tools:style="@android:style/Widget.DeviceDefault.TextView" />

            <CheckBox
                android:id="@+id/suporteMolaHelicoidalQuebrada"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Quebrada" />

            <CheckBox
                android:id="@+id/suporteMolaHelicoidalSolto"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Solto" />

            <TextView
                android:id="@+id/textViewBolsaoAR"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="10dp"
                android:gravity="fill_horizontal|center"
                android:text="Bolsao AR"
                android:textIsSelectable="false"
                android:textSize="15sp"
                android:textStyle="bold"
                tools:layout_marginLeft="30dp"
                tools:style="@android:style/Widget.DeviceDefault.TextView" />

            <CheckBox
                android:id="@+id/bolsaoDeArDanificada"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Danificada" />

            <CheckBox
                android:id="@+id/bolsaoDeArSolto"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Solto" />

            <CheckBox
                android:id="@+id/bolsaoDeArVazando"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Vazando" />

            <TextView
                android:id="@+id/textViewValvulaNivel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="10dp"
                android:gravity="fill_horizontal|center"
                android:text="Valvula Nível"
                android:textIsSelectable="false"
                android:textSize="15sp"
                android:textStyle="bold"
                tools:layout_marginLeft="30dp"
                tools:style="@android:style/Widget.DeviceDefault.TextView" />

            <CheckBox
                android:id="@+id/valculaNivelDanificada"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Danificada" />

            <CheckBox
                android:id="@+id/valculaNivelSolta"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Solto" />

            <CheckBox
                android:id="@+id/valculaNivelVazando"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Vazando" />

            <TextView
                android:id="@+id/textViewBarraEstabilizadora"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="10dp"
                android:gravity="fill_horizontal|center"
                android:text="Barra Estabilizadora"
                android:textIsSelectable="false"
                android:textSize="15sp"
                android:textStyle="bold"
                tools:layout_marginLeft="30dp"
                tools:style="@android:style/Widget.DeviceDefault.TextView" />

            <CheckBox
                android:id="@+id/barraEstabilizadoraSolta"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Solto" />

            <CheckBox
                android:id="@+id/barraEstabilizadoraFalta"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Falta" />

            <CheckBox
                android:id="@+id/barraEstabilizadoraQuebrada"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Quebrada" />

            <CheckBox
                android:id="@+id/barraEstabilizadoraFolga"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Folga" />

            <TextView
                android:id="@+id/textViewBuchaBarraEstabilizadora"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="10dp"
                android:gravity="fill_horizontal|center"
                android:text="Bucha Barra Estabilizadora"
                android:textIsSelectable="false"
                android:textSize="15sp"
                android:textStyle="bold"
                tools:layout_marginLeft="30dp"
                tools:style="@android:style/Widget.DeviceDefault.TextView" />

            <CheckBox
                android:id="@+id/buchaBarraEstabilizadoraFalta"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Falta" />

            <CheckBox
                android:id="@+id/buchaBarraEstabilizadoraFolga"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Folga" />

            <TextView
                android:id="@+id/textViewBananaBean"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="10dp"
                android:gravity="fill_horizontal|center"
                android:text="Banana Bean"
                android:textIsSelectable="false"
                android:textSize="15sp"
                android:textStyle="bold"
                tools:layout_marginLeft="30dp"
                tools:style="@android:style/Widget.DeviceDefault.TextView" />

            <CheckBox
                android:id="@+id/bananaBeanSolta"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Solta" />

            <CheckBox
                android:id="@+id/bananaBeanDanificada"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Danificada" />

            <CheckBox
