                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="10dp"
                android:gravity="fill_horizontal|center"
                android:text="Amortecedor"
                android:textIsSelectable="false"
                android:textSize="15sp"
                android:textStyle="bold"
                tools:layout_marginLeft="30dp"
                tools:style="@android:style/Widget.DeviceDefault.TextView" />

            <CheckBox
                android:id="@+id/amortecedorFaltando"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Faltando" />

            <CheckBox
                android:id="@+id/amortecedorVazando"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Vazando" />

            <CheckBox
                android:id="@+id/amortecedorSolto"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Solto" />

            <CheckBox
                android:id="@+id/amortecedorDanificado"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Danificado" />

            <TextView
                android:id="@+id/textViewSuporteAmortecedor"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="10dp"
                android:gravity="fill_horizontal|center"
                android:text="Suporte Amortecedor"
                android:textIsSelectable="false"
                android:textSize="15sp"
                android:textStyle="bold"
