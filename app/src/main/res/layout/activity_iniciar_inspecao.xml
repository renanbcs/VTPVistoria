<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.IniciarInspecaoActivity"
    tools:layout_editor_absoluteY="25dp">

    <include
        android:id="@+id/toolbar"
        layout="@layout/toolbar" />

    <Button
        android:id="@+id/button_voltar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/editText_prefixo_inciarInpecao"
        android:layout_alignTop="@+id/button_buscarPrefixo"
        android:text="@string/botao_voltar"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="367dp"
        android:onClick="startDialog"/>

    <Button
        android:id="@+id/button_buscarPrefixo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/editText_prefixo_inciarInpecao"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="122dp"
        android:text="@string/botao_proximo"
        tools:layout_editor_absoluteX="204dp"
        tools:layout_editor_absoluteY="367dp" />

    <EditText
        android:id="@+id/editText_prefixo_inciarInpecao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:ems="10"
        android:hint="@string/hint_prefixo"
        android:inputType="number"
        tools:layout_editor_absoluteX="72dp"
        tools:layout_editor_absoluteY="223dp" />

    <TextView
        android:id="@+id/textView_mensagem_iniciarInspecao"
        android:layout_width="213dp"
        android:layout_height="47dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="117dp"
        android:text="@string/mensagem_iniciarInspecao"
        tools:layout_editor_absoluteX="73dp"
        tools:layout_editor_absoluteY="107dp" />


</RelativeLayout>