<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.BuscarPorInspetor.ListaInspetoresActivity"
    tools:layout_editor_absoluteY="81dp"
    android:orientation="vertical"
    android:background="#58C3BF">

    <TextView
        android:id="@+id/textView13"
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:background="@color/fundoimpar"
        android:gravity="center"
        android:text="Lista com os inspetores\nSelecione um para continuar"
        android:textColor="#111"
        android:textSize="20sp" />

    <ListView
        android:id="@+id/listViewInspetores"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</LinearLayout>